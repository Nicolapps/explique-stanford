{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "lib": [
      "ES2021",
    ],
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "isolatedModules": true,
    "skipLibCheck": true,
    "noEmit": true,
  },
}